{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "description": "A simple bar chart with embedded data.",
  "data": {
    "url": "https://raw.githubusercontent.com/TheAnimalOne/FIT3179/main/ausenvironmentaldisasterdata.csv"
  },
  "mark": "bar",
  "encoding": {
    "x": {"field": "title", "type": "nominal", "title": "Disasters"},
    "y": {"aggregate":"sum","field": "Deaths", "type": "quantitative", "title": "Number of deaths"}
  }
}
